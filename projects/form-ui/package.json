{
  "name": "@jaspero/fb-form-ui",
  "version": "2.7.15",
  "description": "Simple form UI for @jaspero/form-builder",
  "author": {
    "name": "Jaspero Ltd",
    "email": "info@jaspero.co"
  },
  "keywords": [
    "Angular"
  ],
  "license": "MIT",
  "homepage": "https://jaspero.co",
  "i18n": [
    {
      "scope": "fb-fu",
      "path": "assets/i18n"
    }
  ],
  "peerDependencies": {
    "@jaspero/form-builder": "^5.9.1",
    "@jaspero/fb-tinymce": "^2.12.5"
  },
  "release": {
    "pkgRoot": "../../dist/@jaspero/fb-form-ui",
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/@jaspero/fb-form-ui"
          ]
        }
      ]
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ]
  }
}
