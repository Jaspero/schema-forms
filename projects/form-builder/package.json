  ],
  "homepage": "https://jaspero.co",
  "peerDependencies": {
    "@angular/common": "^10.0.2",
    "@angular/core": "^10.0.2",
    "@angular/forms": "^10.0.8",
    "@angular/animations": "^10.0.8",
    "@angular/platform-browser": "^10.0.8",
    "@angular/material": "^10.0.1",
    "@jaspero/ng-helpers": "^4.0.2",
    "@ngneat/transloco": "^2.18.4",
