{
  "name": "schema-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently \"npm start\" \"ng build form-builder --watch\"",
    "build:form-builder": "ng build --prod form-builder && cp ./README.md ./dist/@jaspero/form-builder && cp LICENSE ./dist/@jaspero/form-builder",
    "build:page-builder": "ng build --prod page-builder && cp ./README.md ./dist/@jaspero/fb-page-builder && cp LICENSE ./dist/@jaspero/fb-page-builder",
    "build": "npm run build:form-builder && npm run build:page-builder",
    "build:dev:form-builder": "ng build form-builder",
    "build:dev:page-builder": "ng build page-builder",
    "test": "ls",
    "test:app": "ls",
    "lint": "ng lint form-builder && ng lint page-builder",
    "lint:fix": "ng lint form-builder --fix && ng lint page-builder --fix",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "Angular"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.{ts,json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "10.0.14",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.0.14",
    "@angular/compiler": "10.0.14",
    "@angular/core": "10.0.14",
    "@angular/forms": "10.0.14",
    "@angular/material": "10.1.3",
    "@angular/platform-browser": "10.0.14",
    "@angular/platform-browser-dynamic": "10.0.14",
    "@angular/router": "10.0.14",
    "@jaspero/form-builder": "^1.29.0",
    "@jaspero/ng-helpers": "4.0.2",
    "@ngneat/transloco": "2.19.1",
    "json-pointer": "^0.6.0",
    "rxjs": "^6.6.2",
    "tinymce": "^5.4.1",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.7",
    "@angular-devkit/build-ng-packagr": "0.1000.7",
    "@angular/cli": "10.0.7",
    "@angular/compiler-cli": "10.0.14",
    "@angular/language-service": "10.0.14",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/json-pointer": "1.0.30",
    "@types/json-schema": "7.0.4",
    "@types/node": "12.11.1",
    "codelyzer": "5.1.2",
    "concurrently": "5.1.0",
    "husky": "4.2.3",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lint-staged": "10.0.7",
    "ng-packagr": "10.0.4",
    "protractor": "^7.0.0",
    "semantic-release": "17.0.4",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.6"
  },
  "release": {
    "pkgRoot": "dist/@jaspero/form-builder",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/**"
          ]
        }
      ],
      "@semantic-release/git"
    ]
  }
}
